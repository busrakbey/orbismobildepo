<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <stroke android:width="1dip" android:color="#74948B" />
            <gradient android:angle="-90" android:endColor="#689a92" android:startColor="#557C74" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle">
            <stroke android:width="1dip" android:color="#6D8680" />
            <solid android:color="#58857e" />
        </shape>
    </item>
    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android">
            <stroke android:width="1dp" android:color="#D5D3D3" />
            <solid android:color="#FFFFFF" />

        </shape>

    </item>
</selector>